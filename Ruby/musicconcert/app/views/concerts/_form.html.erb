<%= form_for(@concert, html: { multipart: true })do |f| %>
	 <div>
		<%= f.label :band %>
		<br />
		<%= f.text_field :band %>
	</div>
	<div>
		<%= f.label :description %>
		<br />
		<%= f.text_area :description %>
	</div>
	<div>
		<%= f.label :city %>
		<br />
		<%= f.text_field :city %>
	</div>
	<div>
		<%= f.label :venue %>
		<br />
		<%= f.text_field :venue %>
	</div>
	<div>
		<%= f.label :price %>
		<br />
		<%= f.text_field :price %>
	</div>
	<div>
		<%= f.label :date %>
		<br />
		<%= f.date_field :date %>
	</div>
	<br>
	<div>
		<%= f.submit %>
	 </div>
<% end %>